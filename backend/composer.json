{
    "name": "drupalmel/drupalmel",
    "description": "ContentaCMS backend for DrupalMel.",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "contentacms/contenta_jsonapi": "dev-8.x-1.x",
        "cweagans/composer-patches": "^1.6.0",
        "dms/meetup-api-client": "^2.3",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/geolocation": "2.0-beta1",
        "drush/drush": "~8.1.12",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-core-strict": "8.5.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "drupal/coder": "^8.2",
        "drupal/console": "~1.0",
        "drupal/devel": "1.2",
        "drupal/drupal-extension": "~3.0",
        "elkan/behatformatter": "^1.2",
        "jarnaiz/behat-junit-formatter": "^1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "dependency-cleanup": [
            "Drupal\\composer\\ScriptHandler::dependencyCleanup"
        ],
        "drupal-scaffold": "Drupal\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "Drupal\\composer\\ScriptHandler::buildScaffold",
            "Drupal\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "Drupal\\composer\\ScriptHandler::buildScaffold",
            "Drupal\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patches": {}
    },
    "config": {
        "bin-dir": "bin/",
        "preferred-install": {
            "asm89/stack-cors": "source",
            "*": "auto"
        },
        "sort-packages": true
    }
}
